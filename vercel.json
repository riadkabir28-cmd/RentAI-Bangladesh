
{
  "version": 2,
  "headers": [
    {
      "source": "/(.*).(tsx|ts|js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.(js|css|tsx|ts|png|jpg|jpeg|svg|gif|json|ico|woff|woff2|txt)).*)",
      "destination": "/index.html"
    }
  ]
}
